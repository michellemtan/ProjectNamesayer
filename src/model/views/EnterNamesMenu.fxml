<AnchorPane fx:id="mainAnchor" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model.resources.EnterNamesController">
   <children>
      <VBox prefHeight="500.0" prefWidth="800.0">
         <children>
            <GridPane prefHeight="170.0" prefWidth="800.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button mnemonicParsing="false" text="←" GridPane.halignment="LEFT" GridPane.valignment="TOP">
                     <GridPane.margin>
                        <Insets left="15.0" top="15.0" />
                     </GridPane.margin>
                  </Button>
                  <Text fx:id="backButton" onMouseClicked="#backButtonClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Names" textAlignment="CENTER" wrappingWidth="800.33203125">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Text>
               </children>
            </GridPane>
            <SplitPane fx:id="splitPane" dividerPositions="0.005012531328320802" prefHeight="500.0" prefWidth="709.0">
              <items>
                <AnchorPane maxWidth="800.0" minHeight="0.0" minWidth="0.0" prefWidth="421.0">
                     <children>
                        <TextField fx:id="filteredInput" layoutX="35.0" layoutY="46.0" onKeyPressed="#clearTextField" prefHeight="26.0" prefWidth="223.0" promptText="Search..." />
                        <ListView fx:id="databaseNamesListView" layoutX="35.0" layoutY="72.0" prefHeight="321.0" prefWidth="223.0" />
                        <Label fx:id="dbName" layoutX="35.0" layoutY="22.0" text="names_db">
                           <font>
                              <Font name="System Bold" size="13.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
                <AnchorPane fx:id="namesAnchor" maxWidth="800.0" minHeight="0.0" minWidth="0.0" prefHeight="428.0" prefWidth="800.0">
                     <children>
                        <VBox maxWidth="800.0" prefHeight="428.0" prefWidth="790.0" AnchorPane.bottomAnchor="-0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Name:" translateX="50.0" wrappingWidth="147.08984375">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Text>
                              <HBox>
                                 <VBox.margin>
                                    <Insets left="50.0" right="50.0" top="5.0" />
                                 </VBox.margin>
                                 <children>
                                    <Button fx:id="addButton" maxHeight="36.0" minHeight="36.0" minWidth="50.0" mnemonicParsing="false" onMouseClicked="#addButtonClicked" text=" + ">
                                       <font>
                                          <Font size="16.0" />
                                       </font>
                                    </Button>
                                    <TextField fx:id="nameInput" maxWidth="638.0" minWidth="80.0" onKeyPressed="#enterName" prefHeight="36.0" prefWidth="638.0" promptText="e.g. 'Bruce Li'" />
                                 </children>
                              </HBox>
                              <HBox translateY="280.0">
                                 <children>
                                    <Button fx:id="expandButton" mnemonicParsing="false" onAction="#expandButtonClicked" text="◀ Expand" translateX="50.0" />
                                    <Button fx:id="practiceButton" mnemonicParsing="false" onMouseClicked="#practiceButtonClicked" text="Practice" translateX="260.0">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                       <HBox.margin>
                                          <Insets bottom="20.0" />
                                       </HBox.margin>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                        <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="80.0">
                           <children>
                              <ListView fx:id="practiceNamesListView" />
                              <ListView fx:id="transparentListView" prefHeight="200.0" prefWidth="200.0" stylesheets="@../resources/Theme.css" />
                           </children>
                        </StackPane>
                     </children>
                  </AnchorPane>
              </items>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </SplitPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
