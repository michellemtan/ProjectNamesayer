<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="bground" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="model.resources.OverwriteRecordingController">
   <center>
      <Label text="Are you sure you want to overwrite your recording?" textAlignment="CENTER" translateY="3.0" wrapText="true" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <left>
      <Region maxWidth="20.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Region maxWidth="20.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <Label text="Overwrite Recording?" translateY="20.0" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
   </top>
   <bottom>
      <HBox prefHeight="44.0" prefWidth="400.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="yesButton" mnemonicParsing="false" onMouseClicked="#yesButtonClicked" text="Yes" translateX="90.0" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" text="Cancel" translateX="90.0" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
